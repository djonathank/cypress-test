{
  "name": "cypress-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open:cucumber": "npx cypress open",
    "cy:run:cucumber": "npx cypress run --spec **/All.features ",
    "report:merge": "mochawesome-merge > full_report.json",
    "report:mocha": "marge full_report.json",
    "report:clean": "rm -rf mochawesome full_report.json cypress/cucumberJson/**",
    "report:cucumber": "node cypress/cucumberJson/reporter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djonathank/cypress-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djonathank/cypress-test/issues"
  },
  "homepage": "https://github.com/djonathank/cypress-test#readme",
  "devDependencies": {
    "chance": "^1.1.11",
    "cypress": "^13.5.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-multi-reporters": "^1.6.4",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "noGlobalStepDefinitions": true,
    "step_definitions": "cypress/e2e/steps",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
